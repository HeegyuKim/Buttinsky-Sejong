			<sem_grp>
				<sem_class>피동적행위</sem_class>
				<trans>get</trans>
			</sem_grp>
			<frame_grp type="FTR">
				<frame>Y=N1-에 X=N0-이 V</frame>
				<subsense>
					<sel_rst arg="X" tht="THM">(불)</sel_rst>
					<sel_rst arg="Y" tht="LOC">(아궁이|방|방고래)</sel_rst>
