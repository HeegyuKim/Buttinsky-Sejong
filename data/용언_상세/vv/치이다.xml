			<sem_grp>
				<sem_class>피동적행위</sem_class>
				<trans>cost</trans>
				<trans>be valued</trans>
			</sem_grp>
			<frame_grp type="FIN">
				<frame>X=N0-이 Y=N1-이 V</frame>
				<subsense>
					<sel_rst arg="X" tht="THM">구체물</sel_rst>
					<sel_rst arg="Y" tht="FNS">수|단위(원|달러)</sel_rst>
					<n_appr arg="X">값</n_appr>
					<n_appr arg="X">가격</n_appr>
					<n_appr arg="X">금액</n_appr>
					<n_appr arg="X">비용</n_appr>
					<eg>이 집은 값이 1억원도 안 치인다.</eg>
					<eg>이 생선의 가격은 10000원 정도 치이겠다.</eg>
				</subsense>
			</frame_grp>
			<frame_grp type="FIN">
				<frame>X=N0-이 ADV V</frame>
				<subsense>
					<sel_rst arg="X" tht="THM">구체물</sel_rst>
					<sel_rst arg="ADV">(비싸게|싸게)</sel_rst>
					<n_appr arg="X">값</n_appr>
					<n_appr arg="X">가격</n_appr>
					<n_appr arg="X">금액</n_appr>
					<n_appr arg="X">비용</n_appr>
