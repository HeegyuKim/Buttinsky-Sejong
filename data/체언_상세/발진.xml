				<trans>launching</trans>
				<sem_class>출발</sem_class>
				<lr>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_aj type="appr" pred="no">신속한 ~</comb_aj>
				<comb_v type="npred_vsup">
					<form vcompound="yes">~을 하다</form>
					<frame>X-이 ~을 하다</frame>
				</comb_v>
				<comb_v type="npred_vpred">
					<form vcompound="no" >~을 가로막다</form>
				</comb_v>
				<comb_v type="npred_vpred">
					<form vcompound="no">~을 방해하다</form>
				</comb_v>
				<max_n_str>X-의 ~</max_n_str>
				<sel_rst arg="X">교통기관(비행기|배|자동차)</sel_rst>
			</syn_grp>
		</sense>
