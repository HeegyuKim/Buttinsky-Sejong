		<sense n="1">
			<sem_grp>
				<eg>어려울 때일수록 ~가 무엇보다 중요하다.</eg>
				<trans>harmony among men</trans>
				<domain></domain>
				<reg></reg>
				<con></con>
				<sem_class>행위</sem_class> <!--'인지추상적행위'를 '행위'로 수정(송근영)-->
				<lr>
					<syn type="temp"></syn>
					<ant type="temp"></ant>
					<rel></rel>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_aj type="temp"></comb_aj>
				<comb_n type="temp"></comb_n>
				<comb_v type="npred_vsup">
					<form vcompound="yes">~를 하다</form> <!--동일기능동사의 격틀정보를 한 곳으로 모음(송근영)-->
					<frame>X-이 Y-와 ~를 하다</frame>
					<frame>X-와 Y-이 ~를 하다</frame>
				</comb_v>
				<!--vcompound: ~하다-->
				<comb_v type="npred_vsupvar"> <!--기능동사 변이형으로 수정하고 격틀 기술(송근영)-->
					<form vcompound="no">~를 이루다</form>
					<frame>X-이 Y-와 ~를 이루다</frame>
					<frame>X-와 Y-이 ~를 이루다</frame>
				</comb_v>
				<comb_v type="npred_vsupvar"> <!--기능동사 변이형으로 수정하고 격틀 기술(송근영)-->
					<form vcompound="no">~가 이루어지다</form>
					<frame>X-와 Y-의 ~가 이루어지다 </frame>
				</comb_v>
				<max_n_str>X-와 Y-의 ~</max_n_str>
				<sel_rst arg="X">인간|인간집단</sel_rst>
				<sel_rst arg="Y">인간|인간집단</sel_rst>
				<cl type="temp"></cl>
				<prt></prt>
				<av></av>
				<pre_d/>
				<pre_n/>
				<s_n/>
			</syn_grp>
