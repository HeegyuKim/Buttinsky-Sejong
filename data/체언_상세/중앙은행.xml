<superEntry>
	<orth>중앙은행</orth>
	<entry pos="nng_c">
		<mnt_grp>
			<cre>
				<cre_date>2004/09/06</cre_date>
				<cre_writer>권오희</cre_writer>
				<cre_note>n_v검토바람</cre_note>
			</cre>
