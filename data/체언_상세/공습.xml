				<trans>air raid</trans>
				<trans>air attack</trans>
				<domain></domain>
				<reg></reg>
				<con></con>
				<sem_class>방향성행위</sem_class>
				<lr>
					<syn type="temp"></syn>
					<ant type="temp"></ant>
					<rel></rel>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_aj type="temp"></comb_aj>
				<comb_n type="compound">야간 ~</comb_n>
				<comb_n type="compound">~ 경보</comb_n>
				<comb_v type="npred_vpred"> <!--술어명사로 유형 수정(송근영)-->
					<form vcompound="no">~을 단행하다</form>
					<frame></frame>
				</comb_v>
				<comb_v type="npred_vpred"> <!--술어명사로 유형 수정(송근영)-->
					<form vcompound="no">~을 감행하다</form>
					<frame></frame>
				</comb_v>
				<comb_v type="npred_vsup">
					<form vcompound="yes">~을 하다</form>
					<frame>X-이 Y-을 ~을 하다</frame>
					<frame>X-이 Y-에 ~을 하다</frame>
				</comb_v>
				<comb_v type="npred_vsup"> <!--'로부터' 격틀 삭제(송근영)-->
					<form vcompound="no">~을 받다</form>
					<frame>Y-이 X-에게 ~을 받다</frame>
					<frame>Y-이 X-에게서 ~을 받다</frame>
				</comb_v>
					<comb_v type="npred_vsup"> <!--'로부터' 격틀 삭제(송근영)-->
					<form vcompound="no">~을 당하다</form>
					<frame>Y-이 X-에게 ~을 당하다</frame>
					<frame>Y-이 X-에게서 ~을 당하다</frame>
				</comb_v>
				<comb_v type="npred_vsupvar">
					<form vcompound="no">~을 퍼붓다</form>
					<frame>Y-이 X-에게 ~을 퍼붓다</frame>
					<frame>Y-이 X-에게서 ~을 퍼붓다</frame>
				</comb_v>
				<!--vcompound: ~하다-->
				<max_n_str>Y-에 대한 X-의 ~</max_n_str>
				<sel_rst arg="X">인간집단</sel_rst>
				<sel_rst arg="Y">인간집단|장소</sel_rst>
				<cl type="uni">번</cl>
				<cl type="uni">차례</cl>
				<cl type="uni">회</cl>
				<prt></prt>
				<av></av>
				<pre_d/>
				<pre_n/>
				<s_n/>
