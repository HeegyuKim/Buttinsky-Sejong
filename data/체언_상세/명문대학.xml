					<frame></frame>
				</comb_v>
				<max_n_str></max_n_str>
				<sel_rst arg="X"></sel_rst>
				<cl type="uni">곳</cl>
				<cl type="uni">군데</cl>
				<cl type="uni">개교</cl>
				<cl type="uni">개</cl><!--추가(김이정)-->
